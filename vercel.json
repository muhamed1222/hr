{
  "version": 2,
  "name": "hr-admin",
  "builds": [
    {
      "src": "admin-panel/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/admin-panel/$1" }
  ]
} 